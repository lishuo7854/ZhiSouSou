    <label for="name" id="name_li">用户名:</label>
    <input type="text" class="form-control" id="name" 
         placeholder="">
  </div>
    <div class="form-group" style="width: 87%; margin: 0 auto;">
    <label for="name" id="name_li">邮箱:</label>
    <input type="text" class="form-control" id="name" 
         placeholder="">
  </div>
  <div class="form-group" style="width: 87%; margin: 0 auto;">
    <label for="name" id="name_li">联系方式:</label>
    <input type="text" class="form-control" id="name" 
         placeholder="">
  </div>

</form>
  
  
     
        <button type="button" class="btn btn-primary" style="margin-left: 480px; margin-top: 20px;">
          保存
        </button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal -->
</div>



  </body>
</html>
